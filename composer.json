{
    "name": "michelebonacina/phpgpxlib",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "michelebonacina",
        "phpgpxlib"
    ],
    "homepage": "https://github.com/michelebonacina/phpgpxlib",
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Bonacina",
            "email": "michele.bonacina@gmail.com",
            "homepage": "https://github.com/michelebonacina",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MicheleBonacina\\PhpGpxLib\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicheleBonacina\\PhpGpxLib\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
